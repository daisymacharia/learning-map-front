    "express": "^4.15.3",
    "extract-text-webpack-plugin": "^2.1.2",
    "file-loader": "^0.11.2",
    "ignore-styles": "^5.0.1",
    "image-webpack-loader": "^3.3.1",
    "jquery": "^3.2.1",
    "open": "^0.0.5",
    "postcss-cssnext": "^3.0.2",
    "postcss-load-config": "^1.2.0",
    "react": "^15.6.1",
    "react-dom": "15.5+",
    "react-redux": "^5.0.5",
    "react-router": "^4.1.1",
    "react-router-dom": "^4.1.1",
    "react-test-renderer": "^15.6.1",
    "react-toolbox": "^2.0.0-beta.12",
    "redux": "^3.7.1",
