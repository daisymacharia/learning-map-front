{
  "name": "learning-map-front",
  "version": "0.0.0",
  "description": "The Collective Andela Learning Map client side implementation",
  "main": "server.js",
  "repository": "git@github.com:andela/learning-map-front.git",
  "author": "<kesiena.akpobome@andela.com>",
  "license": "MIT",
  "scripts": {
    "start:dev": "npm-run-all --parallel start test:watch",
    "start": "webpack --config=webpack.config.dev.js && babel server.js --out-file server-transpiled.js && node server-transpiled.js",
    "test": "mocha  \"src/**/*.test.tsx\" --compilers ts:ts-node/register,tsx:ts-node/register --require ignore-styles",
    "test:watch": "npm run test -- --watch",
    "clean-dist": "npm run remove-dist && mkdir dist",
    "remove-dist": "node_modules/.bin/rimraf ./dist",
    "copy:html": "cp src/index.html dist/",
    "prebuild": "npm-run-all clean-dist copy:html",
    "build": "webpack --config=webpack.config.prod.js",
    "postbuild": "babel server.js --out-file server-transpiled.js && node server-transpiled.js"
  },
  "devDependencies": {
    "colors": "^1.1.2",
    "expect": "^1.20.2",
    "mocha": "^3.4.2",
    "npm-run-all": "^4.0.2"
  },
  "dependencies": {
    "node-sass": "^4.5.3",
    "postcss": "^6.0.6",
    "ts-node": "^3.1.0",
    "tslint": "^5.4.3",
    "typescript": "^2.4.1",
    "typings": "^2.1.1",
    "tslint-loader": "^3.5.3",
    "postcss-loader": "^2.0.6",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.18.2",
    "ts-loader": "^2.2.2",
    "@types/chai": "^4.0.1",
    "@types/jquery": "^3.2.6",
    "@types/mocha": "^2.2.41",
    "@types/node": "^8.0.14",
    "@types/react": "^15.0.35",
    "@types/react-dom": "^15.5.1",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-loader": "^7.1.1",
    "css-loader": "^0.28.4",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-register": "^6.24.1",
    "chai": "^4.0.2",
    "compression": "^1.7.0",
    "css-loader": "^0.28.4",
    "dotenv": "^4.0.0",
